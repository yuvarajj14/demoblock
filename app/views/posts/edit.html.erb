<div class='row'>
  <div class='col-md-4 col-md-offset-4'>
    <h1 class="text-center">Edit Post</h1>
    <%= form_for :post, html: { multipart: true }, url: post_path(@post), class:"form col-md-12 center-block", method: :PATCH do |f| %>
      <%= render partial: "form", locals: {f: f} %>
    <% end %>
    <%= link_to 'Show', @post %> |
    <%= link_to 'Back', posts_path %>
  </div>
</div>
